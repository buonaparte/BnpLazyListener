{
    "name": "buonaparte/bnp-lazy-listener",
    "description": "This library allows you to attach Lazy Listeners to a ZF2 EventManager",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "eventmanager",
        "lazy"
    ],
    "authors": [
        {
            "name": "Denis Mostovoi",
            "email": "buonaparte89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-servicemanager": "2.*",
        "zendframework/zend-eventmanager": "2.*"
    },
    "require-dev" : {
        "phpunit/phpunit" : ">=3.7,<4",
        "squizlabs/php_codesniffer" : "1.4.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "BnpLazyListener\\": "src/"
        }
    }
}